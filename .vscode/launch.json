{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SageStore: Debug Server executable",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/_server/SageStoreServer",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "SageStore: Build: Compile current configuration",
